{
	"name": "starries",
	"version": "0.0.1",
	"scripts": {
		"dev": "npm-run-all -p serverup svelte:dev",
		"serverup": "./tmp/starry",
		"svelte:dev": "svelte-kit dev",
		"tauri:dev": "npm run tauri dev",
		"release": "npm run tauri build",
		"preview": "npm run build && npm run tauri:dev",
		"build": "svelte-kit build",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"tauri": "tauri",
		"postinstall": "node scripts/postinstall.js && husky install && husky set .husky/pre-commit 'npm run check && npm run format'"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.13",
		"@sveltejs/kit": "next",
		"@tauri-apps/api": "^1.0.0-beta.1",
		"@tauri-apps/cli": "^1.0.0-beta.2",
		"@typescript-eslint/eslint-plugin": "^4.27.0",
		"@typescript-eslint/parser": "^4.27.0",
		"adm-zip": "^0.5.5",
		"download": "^8.0.0",
		"eslint": "^7.28.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"husky": "^6.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.3.1",
		"prettier-plugin-svelte": "^2.3.0",
		"svelte": "^3.38.2",
		"svelte-check": "^2.1.0",
		"svelte-preprocess": "^4.7.3",
		"tslib": "^2.3.0",
		"typescript": "^4.3.3"
	},
	"type": "module"
}
